{
	"from": {
		"prefix": "from",
		"body": [
			"from ${1: } import ${2: }"
		]
	},

	"import": {
		"prefix": "import",
		"body": [
			"import ${1: } as ${2: }"
		]
	},

	"input": {
		"prefix": "input",
		"body": [
			"input()$1"
		]
	},
	
	"split": {
		"prefix": "split",
		"body": [
			"split()$1"
		]
	},

	"map": {
		"prefix": "map",
		"body": [
			"map(${1:int}, ${2:input().split()})$3"
		]
	},

	"list": {
		"prefix": "list",
		"body": [
			"list(${1: })"
		]
	},

	"int": {
		"prefix": "int",
		"body": [
			"int(${1: })"
		]
	},

	"range": {
		"prefix": "range",
		"body": [
			"range(${1:N})"
		]
	},

	"for": {
		"prefix": "for",
		"body": [
			"for ${1:i} in range(${2:N}):",
			"    $3"
		]
	},

	"while": {
		"prefix": "while",
		"body": [
			"while ${1: }:",
			"    $2"
		]
	},

	"if": {
		"prefix": "if",
		"body": [
			"if ${1: }:",
			"    $2"
		]
	},

	"elif": {
		"prefix": "elif",
		"body": [
			"elif ${1: }:",
			"    $2"
		]
	},

	"else": {
		"prefix": "else",
		"body": [
			"else:",
			"    $1"
		]
	},

	"yes": {
		"prefix": "yes",
		"body": [
			"print(\"Yes\")"
		]
	},

	"no": {
		"prefix": "no",
		"body": [
			"print(\"No\")"
		]
	},

	"print": {
		"prefix": "print",
		"body": [
			"print(${1: })"
		]
	},

	"return": {
		"prefix": "return",
		"body": [
			"return ${1: }"
		]
	},

	"continue": {
		"prefix": "continue",
		"body": [
			"continue"
		]
	},

	"break": {
		"prefix": "break",
		"body": [
			"break"
		]
	},

	"append": {
		"prefix": "append",
		"body": [
			"append(${1: })"
		]
	},

	"sort": {
		"prefix": "sort",
		"body": [
			"sort($1)"
		]
	},

	"search": {
		"prefix": "search",
		"body": [
			"search($1)"
		]
	},

	"fullmatch": {
		"prefix": "fullmatch",
		"body": [
			"fullmatch($1)"
		]
	}

}